        "@types/node": "^24.10.1",
        "@types/react": "^19.2.5",
        "@types/react-dom": "^19.2.3",
        "@types/react-router-dom": "^5.3.3",
        "@vitejs/plugin-react": "^5.1.1",
        "gh-pages": "^6.3.0",
        "typescript": "~5.9.3",
        "vite": "^7.2.4"
    }
}
